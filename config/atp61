nst delTk=_checkSubPagesItem(_isSub,_tkId,keys.bletrntkspgs,keys.bletrntkspg,_skuId,_trnId,_frm);Utils.assert(delTk!==-1,_throwErr(error.TK_NOT_IN_BLE));if(_isSub===true){if(delTk==='0'){_checkSubPagesItem(true,_trnId,keys.bletrnspgs,keys.bletrnspg,_skuId,_frm);}_checkSubPagesItem(true,_tkId,keys.bletkspgs,keys.bletkspg,_skuId,_frm);}}function _transferFrom(_frm,_to,_tkId){const tkKey=_makeKey(keys.tk,_tkId);const tkVal=_checkExist(tkKey,error.TK_NOT_EST);let tk=_toJsn(tkVal);_checkDefaultTranche(tk.skuId,tk.trnId,false);_checkSubBalance(gMsgSender,_frm,tk.skuId,tk.trancheId,_tkId,true);_checkAddBalance(_to,tk.skuId,tk.trancheId,_tkId);}function _transfer(_frm,_to,_tkId){Utils.assert(_checkAddr(_to),_throwErr(error.TO_ERR));Utils.assert(_checkStr(_tkId,1,64),_throwErr(error.TK_ID_ERR));if(_frm===_to){return;}_transferFrom(_frm,_to,_tkId);}function _destroy(_addr,_tkId){const tkKey=_makeKey(keys.tk,_tkId);const tkVal=_checkExist(tkKey,error.TK_NOT_EST);let tk=_toJsn(tkVal);_checkDefaultTranche(tk.skuId,tk.trnId,true);_checkSubBalance(_addr,_addr,tk.skuId,tk.trancheId,_tkId,true);const delSkuTrnTks=_checkSubPagesItem(true,_tkId,keys.skutrntkspgs,keys.skutrntkspg,tk.skuId,tk.trancheId);if(delSkuTrnTks==='0'){_checkSubPagesItem(true,tk.trancheId,keys.skutrnspgs,keys.skutrnspg,tk.skuId);}const skuTkKey=_makeKey(keys.sku,tk.skuId);const skuTkValue=_load(skuTkKey);let skuTk=_toJsn(skuTkValue);skuTk.totalSupply=Utils.int64Sub(skuTk.totalSupply,1);_store(skuTkKey,_toStr(skuTk));}function sellerInfo(){return _load(keys.sel);}function setDocument(id,name,url,hashType,hash){Utils.assert(_checkStr(id,1,32),_throwErr(error.DOC_ID_ERR));Utils.assert(_checkStr(name,1,1024),_throwErr(error.NAME_ERR));Utils.assert(_checkStr(url,1,10240),_throwErr(error.DOC_URL_ERR));Utils.assert(_checkStr(hashType,1,64),_throwErr(error.HASH_TYPE_ERR));Utils.assert(_checkStr(hash,1,2048),_throwErr(error.HASH_ERR));const isSel=_checkIsSeller(gTxSender);Utils.assert(isSel!==false,_throwErr(error.NOT_SEL));Utils.assert(_checkAddPagesItem(true,id,keys.docspgs,keys.docspg)!==-3,_throwErr(error.DOCS_MORE_ERR));let docInfo={};docInfo.name=name;docInfo.url=url;docInfo.hashType=hashType;docInfo.hash=hash;docInfo.provider=gTxSender;docInfo.date=gBlockTime;_store(_makeKey(keys.doc,id),_toStr(docInfo));Chain.tlog('setDocument',_makeTlogSender(),id,name);}function allDocuments(){const docs=_getPagesItems(keys.docspgs,keys.docspg);return _toStr(docs);}function documentInfo(docId){Utils.assert(_checkStr(docId,1,32),_throwErr(error.DOC_ID_ERR));const docVal=_checkExist(_makeKey(keys.doc,docId),error.DOC_NOT_EST);return docVal;}function createSpu(id,name,type,attrs){Utils.assert(_checkStr(id,1,32),_throwErr(error.SPU_ID_ERR));Utils.assert(_checkStr(name,1,1024),_throwErr(error.NAME_ERR));Utils.assert(_checkStr(type,1,64),_throwErr(error.SPU_TYPE_ERR));Utils.assert(_checkIsSeller(gTxSender),_throwErr(error.NOT_SEL));const spuKey=_makeKey(keys.spu,id);_checkNotExist(spuKey,error.SPU_EST);let spu={};spu.name=name;spu.type=type;spu.attributes=attrs;_store(spuKey,_toStr(spu));Chain.tlog('createSpu',_makeTlogSender(),id,name,type);}function setSpu(spuId,name,type,attrs){Utils.assert(_checkStr(spuId,1,32),_throwErr(error.SPU_ID_ERR));Utils.assert(_checkStr(name,1,1024),_throwErr(error.NAME_ERR));Utils.assert(_checkStr(type,1,64),_throwErr(error.SPU_TYPE_ERR));Utils.assert(_checkIsSeller(gTxSender),_throwErr(error.NOT_SEL));const spuKey=_makeKey(keys.spu,spuId);const spuVal=_checkExist(spuKey,error.SPU_NOT_EST);let spu=_toJsn(spuVal);spu.name=name;spu.type=type;spu.attributes=attrs;_store(spuKey,_toStr(spu));Chain.tlog('setSpu',_makeTlogSender(),spuId,name,type);}function spuInfo(spuId){Utils.assert(_checkStr(spuId,1,32),_throwErr(error.SPU_ID_ERR));const spuVal=_checkExist(_makeKey(keys.spu,spuId),error.SPU_NOT_EST);return spuVal;}function createTranche(id,des,lms){Utils.assert(_checkStr(id,1,32),_throwErr(error.TRN_ID_ERR));Utils.assert(_checkStr(des,0,64000),_throwErr(error.DES_ERR));Utils.assert(_checkIsSeller(gTxSender),_throwErr(error.NOT_SEL));const trnKey=_makeKey(keys.trn,id);_checkNotExist(trnKey,error.TRN_EST);let trm={};trm.description=des;trm.limits=lms;_store(trnKey,_toStr(trm));Chain.tlog('createTranche',_makeTlogSender(),id);}function trancheInfo(trnId){trnId=_checkTranche(trnId,0,0,true);const trnVal=_checkExist(_makeKey(keys.trn,trnId),error.TRN_NOT_EST);return trnVal;}function issue(skuId,trnId,isDftTrn,spuId,name,symbol,faceVal,tkId,tkInfo,mainIcn,viceIcns,labels,des,repnAddr,acpId,abs,attrs){trnId=_checkTranche(trnId);Utils.assert(_checkStr(skuId,1,32),_throwErr(error.SKU_ID_ERR));Utils.assert(isDftTrn===undefined||typeof isDftTrn==='boolean',_throwErr(error.IS_DFT_TRN_ERR));Utils.assert(_checkStr(spuId,0,32),_throwErr(error.SPU_ID_ERR));Utils.assert(_checkStr(name,1,1024),_throwErr(error.NAME_ERR));Utils.assert(_checkStr(symbol,1,16),_throwErr(error.TK_BML_ERR));Utils.assert(_checkStr(faceVal,0,32),_throwErr(error.FC_ERR));Utils.assert(_checkStr(tkId,1,64),_throwErr(error.TK_ID_ERR));Utils.assert(_checkStr(tkInfo,1,1024),_throwErr(error.TK_INFO_ERR));Utils.assert(_checkStr(mainIcn,0,10240),_throwErr(error.MAIN_ICN_ERR));Utils.assert(_checkJSNArr(viceIcns,0,5),_throwErr(error.VICE_ICNS_ERR));Utils.assert(_checkStr(des,0,64000),_throwErr(error.DES_ERR));Utils.assert(_checkJSNArr(labels,0,20),_throwErr(error.LBL_ERR));Utils.assert(_checkAddr(repnAddr),_throwErr(error.REPN_ADDR_ERR));Utils.assert(_checkJSNArr(abs,0,20),_throwErr(error.ABT_ERR));Utils.assert(_checkStr(acpId,1,32),_throwErr(error.ACP_ID_ERR));Utils.assert(_checkIsSeller(gTxSender),_throwErr(error.NOT_SEL));_checkNotExist(_makeKey(keys.tk,tkId),error.TK_EST);_checkArrayItem(viceIcns,1,10240,'vice',error.VICE_ICNS_ERR);_checkArrayItem(labels,1,1024,'label',error.LBL_ERR);_checkArrayItem(abs,1,32,'attribute',error.ABT_ERR);_checkNotExist(_makeKey(keys.sku,skuId),error.SKU_EST);if(spuId!==undefined){_checkExist(_makeKey(keys.spu,spuId),error.SPU_NOT_EST);}_checkExist(_makeKey(keys.acp,acpId),error.ACP_NOT_EST);let sku={};sku.spuId=spuId;if(isDftTrn===true){sku.defaultTrancheId=trnId;}sku.name=name;sku.symbol=symbol;sku.faceValue=faceVal;sku.totalSupply=1;sku.description=des;sku.mainIcon=mainIcn;sku.viceIcons=viceIcns;sku.labels=labels;sku.redemptionAddress=repnAddr;sku.attributes=attrs;sku.abstracts=abs;sku.authorizers=[];sku.time=Chain.block.timestamp;_store(_makeKey(keys.sku,skuId),_toStr(sku));let tk={};tk.skuId=skuId;tk.trancheId=trnId;tk.information=tkInfo;Chain.store(_makeKey(keys.tk,tkId),_toStr(tk));_checkAddPagesItem(true,tkId,keys.skutrntkspgs,keys.skutrntkspg,skuId,trnId);_checkAddPagesItem(true,trnId,keys.skutrnspgs,keys.skutrnspg,skuId);_checkAddPagesItem(true,acpId,keys.skuacpspgs,keys.skuacpspg,skuId);_checkAddPagesItem(true,skuId,keys.trnskuspgs,keys.trnskuspg,trnId);if(spuId!==undefined){_checkAddPagesItem(true,skuId,keys.spuskuspgs,keys.spuskuspg,spuId);}_checkAddPagesItem(true,tkId,keys.bletkspgs,keys.bletkspg,skuId,gTxSender);_checkAddPagesItem(true,tkId,keys.bletrntkspgs,keys.bletrntkspg,skuId,trnId,gTxSender);_checkAddPagesItem(true,trnId,keys.bletrnspgs,keys.bletrnspg,skuId,gTxSender);Chain.tlog('issue',_makeTlogSender(),skuId,`${trnId}_${spuId}`,symbol,tkId);}function setSkusChoice(spuId,choice){Utils.assert(_checkStr(spuId,1,32),_throwErr(error.SPU_ID_ERR));Utils.assert(_checkJSNObj(choice)&&_checkStr(_toStr(choice),1,1024),_throwErr(error.COC_ERR));Utils.assert(_checkIsSeller(gTxSender),_throwErr(error.NOT_SEL));const spuKey=_makeKey(keys.spu,spuId);const spuVal=_checkExist(spuKey,error.SPU_NOT_EST);let spu=JSON.parse(spuVal);spu.choice=choice;_store(spuKey,_toStr(spu));Chain.tlog('setSkusChoice',_makeTlogSender(),spuId,_toStr(choice));}function setSku(skuId,name,symbol,mainIcn,viceIcns,labels,des,repnAddr,abs,attrs){Utils.assert(_checkStr(skuId,1,32),_throwErr(error.SKU_ID_ERR));Utils.assert(_checkStr(name,0,1024),_throwErr(error.NAME_ERR));Utils.assert(_checkStr(symbol,0,16),_throwErr(error.TK_BML_ERR));Utils.assert(_checkStr(mainIcn,0,10240),_throwErr(error.MAIN_ICN_ERR));Utils.assert(_checkJSNArr(viceIcns,0,5),_throwErr(error.VICE_ICNS_ERR));Utils.assert(_checkStr(des,0,64000),_throwErr(error.DES_ERR));Utils.assert(_checkJSNArr(labels,0,20),_throwErr(error.LBL_ERR));Utils.assert(repnAddr===undefined||_checkAddr(repnAddr),_throwErr(error.REPN_ADDR_ERR));Utils.assert(_checkJSNArr(abs,0,20),_throwErr(error.ABT_ERR));Utils.assert(_checkIsSeller(gTxSender),_throwErr(error.NOT_SEL));_checkArrayItem(viceIcns,1,10240,'vice',error.VICE_ICNS_ERR);_checkArrayItem(labels,1,1024,'label',error.LBL_ERR);_checkArrayItem(abs,1,32,'attribute',error.ABT_ERR);const skuTkKey=_makeKey(keys.sku,skuId);const skuTkVal=_checkExist(skuTkKey,error.SKU_NOT_EST);let skuTk=_toJsn(skuTkVal);if(name!==undefined){skuTk.name=name;}if(symbol!==undefined){skuTk.symbol=symbol;}if(mainIcn!==undefined){skuTk.mainIcon=mainIcn;}if(viceIcns!==undefined){skuTk.viceIcons=viceIcns;}if(labels){skuTk.labels=labels;}if(des!==undefined){skuTk.description=des;}if(repnAddr!==undefined){skuTk.redemptionAddress=repnAddr;}if(attrs!==undefined){skuTk.attributes=attrs;}if(abs!==undefined){skuTk.abstracts=abs;}_store(skuTkKey,_toStr(skuTk));Chain.tlog('modifySku',_makeTlogSender(),skuId);}function addAcceptanceToSku(skuId,acpId){Utils.assert(_checkStr(skuId,1,32),_throwErr(error.SKU_ID_ERR));Utils.assert(_checkStr(acpId,1,32),_throwErr(error.ACP_ID_ERR));Utils.assert(_checkIsSeller(gTxSender),_throwErr(error.NOT_SEL));const skuTkKey=_makeKey(keys.sku,skuId);_checkExist(skuTkKey,error.SKU_NOT_EST);_checkExist(_makeKey(keys.acp,acpId),error.ACP_NOT_EST);const ret=_checkAddPagesItem(true,acpId,keys.skuacpspgs,keys.skuacpspg,skuId);Utils.assert(ret!==-2,_throwErr(error.ACP_EST_SKU_ERR));Utils.assert(ret!==-3,_throwErr(error.ACP_MORE_SKU_ERR));Chain.tlog('addAcceptanceToSku',_makeTlogSender(),skuId,acpId);}function delAcceptanceFromSku(skuId,acpId){Utils.assert(_checkStr(skuId,1,32),_throwErr(error.SKU_ID_ERR));Utils.assert(_checkStr(acpId,1,32),_throwErr(error.ACP_ID_ERR));Utils.assert(_checkIsSeller(gTxSender),_throwErr(error.NOT_SEL));const skuTkKey=_makeKey(keys.sku,skuId);_checkExist(skuTkKey,error.SKU_NOT_EST);const ret=_checkSubPagesItem(true,acpId,keys.skuacpspgs,keys.skuacpspg,skuId);Utils.assert(ret!==-1,_throwErr(error.ACP_NOT_EST_SKU_ERR));Chain.tlog('DelAcceptanceFromSku',_makeTlogSender(),skuId,acpId);}function additionalIssuance(skuId,trnId,tkId,tkInfo){Utils.assert(_checkStr(skuId,1,32),_throwErr(error.SKU_ID_ERR));Utils.assert(_checkStr(tkId,1,64),_throwErr(error.TK_ID_ERR));Utils.assert(_checkStr(tkInfo,1,1024),_throwErr(error.TK_INFO_ERR));Utils.assert(_checkIsSeller(gTxSender),_throwErr(error.NOT_SEL));_checkNotExist(_makeKey(keys.tk,tkId),error.TK_EST);const skuTkKey=_makeKey(keys.sku,skuId);const skuTkVal=_checkExist(skuTkKey,error.SKU_NOT_EST);let skuTk=_toJsn(skuTkVal);skuTk.totalSupply=Utils.int64Add(skuTk.totalSupply,1);_store(skuTkKey,_toStr(skuTk));trnId=_checkTranche(trnId,skuTk.defaultTrancheId,true);const ret=_checkAddPagesItem(true,tkId,keys.skutrntkspgs,keys.skutrntkspg,skuId,trnId);Utils.assert(ret!==-3,_throwErr(error.TK_MORE_TRN_ERR));_checkAddPagesItem(true,trnId,keys.skutrnspgs,keys.skutrnspg,skuId);let tk={};tk.skuId=skuId;tk.trancheId=trnId;tk.information=tkInfo;Chain.store(_makeKey(keys.tk,tkId),_toStr(tk));_checkAddPagesItem(true,tkId,keys.bletkspgs,keys.bletkspg,skuId,gTxSender);_checkAddPagesItem(true,tkId,keys.bletrntkspgs,keys.bletrntkspg,skuId,trnId,gTxSender);_checkAddPagesItem(true,trnId,keys.bletrnspgs,keys.bletrnspg,skuId,gTxSender);Chain.tlog('additionalIssuance',_makeTlogSender(),skuId,tkId);}function assignToTranche(skuId,toTrnId,tkId){toTrnId=_checkTranche(toTrnId);Utils.assert(_checkStr(skuId,1,32),_throwErr(error.SKU_ID_ERR));Utils.assert(_checkStr(tkId,1,64),_throwErr(error.TK_ID_ERR));const isSel=_checkIsSeller(gTxSender);Utils.assert(isSel,_throwErr(error.NOT_SEL));const skuTkKey=_makeKey(keys.sku,skuId);const skuTkVal=_chec