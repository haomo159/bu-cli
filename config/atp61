Id,trnId);}function authorizedSku(skuId,trnId,autr){Utils.assert(_checkStr(skuId,1,32),_throwErr(error.SKU_ID_ERR));const skuTkKey=_makeKey(keys.sku,skuId);const skuTkValue=_checkExist(skuTkKey,error.SKU_NOT_EST);let skuTk=_toJsn(skuTkValue);trnId=_checkTranche(trnId,skuTk.defaultTrancheId,true);const idx=skuTk.authorizers.indexOf(autr);Utils.assert(idx!==-1,_throwErr(error.ATOR_NOT_EST));const tks=_getPagesItems(keys.skutrnauthtkspgs,keys.skutrnauthtkspg,skuId,trnId,autr);return _toStr(tks);}function skusOfSpu(spuId){Utils.assert(_checkStr(spuId,1,32),_throwErr(error.SPU_ID_ERR));_checkExist(_makeKey(keys.spu,spuId),error.SPU_NOT_EST);const skus=_getPagesItems(keys.spuskuspgs,keys.spuskuspg,spuId);return _toStr(skus);}function skusOfTranche(trnId){Utils.assert(_checkStr(trnId,0,32),_throwErr(error.TRN_ID_ERR));trnId=_checkTranche(trnId);const skus=_getPagesItems(keys.trnskuspgs,keys.trnskuspg,trnId);return _toStr(skus);}function tranchesOfSku(skuId){Utils.assert(_checkStr(skuId,1,32),_throwErr(error.SKU_ID_ERR));const skuTkKey=_makeKey(keys.sku,skuId);_checkExist(skuTkKey,error.SKU_NOT_EST);const trns=_getPagesItems(keys.skutrnspgs,keys.skutrnspg,skuId);return _toStr(trns);}function tranchesOf(skuId,addr){Utils.assert(_checkStr(skuId,1,32),_throwErr(error.SKU_ID_ERR));Utils.assert(_checkAddr(addr),_throwErr(error.ADDR_ERR));const skuTkKey=_makeKey(keys.sku,skuId);_checkExist(skuTkKey,error.SKU_NOT_EST);const trns=_getPagesItems(keys.bletrnspgs,keys.bletrnspg,skuId,addr);return _toStr(trns);}function acceptancesOfSku(skuId){Utils.assert(_checkStr(skuId,1,32),_throwErr(error.SKU_ID_ERR));const skuTkKey=_makeKey(keys.sku,skuId);_checkExist(skuTkKey,error.SKU_NOT_EST);const acps=_getPagesItems(keys.skuacpspgs,keys.skuacpspg,skuId);return _toStr(acps);}function tokenInfo(skuId){Utils.assert(_checkStr(skuId,1,32),_throwErr(error.SKU_ID_ERR));const skuTkVal=_checkExist(_makeKey(keys.sku,skuId),error.SKU_NOT_EST);return skuTkVal;}function totalSupplyByTranche(skuId,trnId){Utils.assert(_checkStr(skuId,1,32),_throwErr(error.SKU_ID_ERR));_checkExist(_makeKey(keys.sku,skuId),error.SKU_NOT_EST);trnId=_checkTranche(trnId);const tks=_getPagesItems(keys.skutrntkspgs,keys.skutrntkspg,skuId,trnId);return _toStr(tks);}function balanceOf(addr,skuId){Utils.assert(_checkStr(skuId,1,32),_throwErr(error.SKU_ID_ERR));Utils.assert(_checkAddr(addr),_throwErr(error.ADDR_ERR));_checkExist(_makeKey(keys.sku,skuId),error.SKU_NOT_EST);const tks=_getPagesItems(keys.bletkspgs,keys.bletkspg,skuId,addr);return _toStr(tks);}function balanceOfByTranche(addr,skuId,trnId){Utils.assert(_checkStr(skuId,1,32),_throwErr(error.SKU_ID_ERR));Utils.assert(_checkAddr(addr),_throwErr(error.ADDR_ERR));_checkExist(_makeKey(keys.sku,skuId),error.SKU_NOT_EST);trnId=_checkTranche(trnId);const tks=_getPagesItems(keys.bletrntkspgs,keys.bletrntkspg,skuId,trnId,addr);return _toStr(tks);}function destroy(address,tkId){Utils.assert(_checkAddr(address),_throwErr(error.ADDR_ERR));Utils.assert(_checkStr(tkId,1,64),_throwErr(error.TK_ID_ERR));const isSel=_checkIsSeller(gTxSender);Utils.assert(isSel!==false,_throwErr(error.NOT_SEL));Utils.assert(address===gTxSender,_throwErr(error.SEL_RDM_OTR));_destroy(address,tkId);Chain.tlog('destroy',_makeTlogSender(),address,tkId);}function approve(spr,tkId){_approve(spr,tkId);Chain.tlog('approve',_makeTlogSender(),spr,tkId);}function allowance(owr,skuId,trnId,spr){Utils.assert(_checkAddr(owr),_throwErr(error.ONR_ERR));Utils.assert(_checkStr(skuId,1,32),_throwErr(error.SKU_ID_ERR));Utils.assert(_checkAddr(spr),_throwErr(error.SPR_ERR));_checkExist(_makeKey(keys.sku,skuId),error.SKU_NOT_EST);trnId=_checkTranche(trnId);const tks=_getPagesItems(keys.alwtrntkspgs,keys.alwtrntkspg,owr,skuId,trnId,spr);return _toStr(tks);}function transferFrom(frm,to,tkId){Utils.assert(_checkAddr(frm),_throwErr(error.FRM_ERR));_transfer(frm,to,tkId);Chain.tlog('transferFrom',_makeTlogSender(),frm,to,tkId);}function transfer(to,tkId){_transfer(gMsgSender,to,tkId);Chain.tlog('transfer',_makeTlogSender(),to,tkId);}function setAcceptance(id,pub,flNm,stNm,logo,cat,period,addi){Utils.assert(_checkStr(id,1,32),_throwErr(error.ACP_ID_ERR));const addr=Utils.toAddress(pub);Utils.assert(_checkAddr(addr),_throwErr(error.PUB_ERR));Utils.assert(_checkStr(flNm,1,1024),_throwErr(error.FL_NM_ERR));Utils.assert(_checkStr(stNm,1,64),_throwErr(error.ST_NM_ERR));Utils.assert(_checkStr(logo,0,10240),_throwErr(error.LOGO_ERR));Utils.assert(_checkStr(cat,1,64),_throwErr(error.CAT_ERR));Utils.assert(Utils.stoI64Check(period)&&Utils.int64Compare(period,0)>0,_throwErr(error.PRD_ERR));const isSel=_checkIsSeller(gTxSender);Utils.assert(isSel,_throwErr(error.NOT_SEL));const acpKey=_makeKey(keys.acp,id);let acp={};acp.publicKey=pub;acp.fullName=flNm;acp.shortName=stNm;acp.logo=logo;acp.contact=cat;acp.period=period;acp.addition=addi;_store(acpKey,_toStr(acp));}function acceptanceInfo(acpId){Utils.assert(_checkStr(acpId,1,32),_throwErr(error.ACP_ID_ERR));const acpKey=_makeKey(keys.acp,acpId);const acpVal=_checkExist(acpKey,error.ACP_NOT_EST);return acpVal;}function requestRedemption(repnId,tkId,acpId,des,addi){Utils.assert(_checkStr(repnId,1,32),_throwErr(error.REPN_ID_ERR));Utils.assert(_checkStr(tkId,1,64),_throwErr(error.TK_ID_ERR));Utils.assert(_checkStr(acpId,1,32),_throwErr(error.ACP_ID_ERR));Utils.assert(_checkStr(des,0,64000),_throwErr(error.DES_ERR));const tkKey=_makeKey(keys.tk,tkId);const tkVal=_checkExist(tkKey,error.TK_NOT_EST);let tk=_toJsn(tkVal);const repnKey=_makeKey(keys.repn,repnId,gMsgSender);_checkNotExist(repnKey,error.REPN_EST);const acpKey=_makeKey(keys.acp,acpId);_checkExist(acpKey,error.ACP_NOT_EST);_checkDefaultTranche(tk.skuId,tk.trnId,false);_checkSubBalance(gMsgSender,gMsgSender,tk.skuId,tk.trancheId,tkId);let repn={};repn.tokenId=tkId;repn.acceptanceId=acpId;repn.status=0;repn.description=des;repn.requestTime=Chain.block.timestamp;repn.addition=addi;_store(repnKey,_toStr(repn));Chain.tlog('requestRedemption',_makeTlogSender(),repnId,tkId);}function redeem(repnId,apt){Utils.assert(_checkStr(repnId,1,32),_throwErr(error.REPN_ID_ERR));Utils.assert(_checkAddr(apt),_throwErr(error.APT_ERR));const repnKey=_makeKey(keys.repn,repnId,apt);const repnVal=_checkExist(repnKey,error.REPN_NOT_EST);let repn=_toJsn(repnVal);Utils.assert(repn.status===0,_throwErr(error.REPN_SAS_ERR));const acpKey=_makeKey(keys.acp,repn.acceptanceId);const acpVal=_load(acpKey);const acp=_toJsn(acpVal);const acpAddr=Utils.toAddress(acp.publicKey);Utils.assert(gTxSender===acpAddr,_throwErr(error.NOT_APR));let dptKey=_makeKey(keys.dpt,repnId,apt);_checkNotExist(dptKey,error.REPN_IN_DPT);repn.status=1;repn.sendTime=gBlockTime;_store(repnKey,_toStr(repn));Chain.tlog('redeem',_makeTlogSender(),repnId,apt);}function confirmRedemption(repnId,apt){Utils.assert(_checkStr(repnId,1,32),_throwErr(error.REPN_ID_ERR));Utils.assert(_checkAddr(apt),_throwErr(error.APT_ERR));const repnKey=_makeKey(keys.repn,repnId,apt);const repnVal=_checkExist(repnKey,error.REPN_NOT_EST);let repn=_toJsn(repnVal);Utils.assert(repn.status===1,_throwErr(error.REPN_SAS_ERR));const acpKey=_makeKey(keys.acp,repn.acceptanceId);const acpVal=_load(acpKey);const acp=_toJsn(acpVal);const isExired=_checkExpired(acp.period,repn.sendTime);if(!isExired){Utils.assert(gMsgSender===apt,_throwErr(error.NOT_APT));}let dptKey=_makeKey(keys.dpt,repnId,apt);_checkNotExist(dptKey,error.REPN_IN_DPT);repn.status=2;repn.finishTime=gBlockTime;_store(repnKey,_toStr(repn));Chain.tlog('confirmRedemption',_makeTlogSender(),repnId,apt);}function redemptionInfo(repnId,apt){Utils.assert(_checkStr(repnId,1,32),_throwErr(error.REPN_ID_ERR));Utils.assert(_checkAddr(apt),_throwErr(error.APT_ERR));const repnKey=_makeKey(keys.repn,repnId,apt);const repnVal=_checkExist(repnKey,error.REPN_NOT_EST);return repnVal;}function applyDispute(repnId,apt,rsn,ctr,addi){Utils.assert(_checkStr(repnId,1,32),_throwErr(error.REPN_ID_ERR));Utils.assert(_checkAddr(apt),_throwErr(error.APT_ERR));Utils.assert(_checkStr(rsn,1,64000),_throwErr(error.RSN_ERR));Utils.assert(_checkAddr(ctr),_throwErr(error.CTR_ERR));Utils.assert(_checkIsSeller(gMsgSender)||gMsgSender===apt,_throwErr(error.NOT_SEL_APT));const repnVal=_checkExist(_makeKey(keys.repn,repnId,apt),error.REPN_NOT_EST);const repn=_toJsn(repnVal);Utils.assert(repn.status===0||repn.status===1,_throwErr(error.SAS_FOR_DPT_ERR));const dptKey=_makeKey(keys.dpt,repnId,apt);_checkNotExist(dptKey,error.DPT_EST);let dpt={};dpt.applicant=gMsgSender;dpt.reason=rsn;dpt.status=0;dpt.controller=ctr;dpt.time=Chain.block.timestamp;dpt.addition=addi;_store(dptKey,_toStr(dpt));Chain.tlog('applyDispute',_makeTlogSender(),repnId,apt,rsn);}function setEvidence(repnId,apt,des,addi){Utils.assert(_checkStr(repnId,1,32),_throwErr(error.REPN_ID_ERR));Utils.assert(_checkAddr(apt),_throwErr(error.APT_ERR));Utils.assert(_checkStr(des,0,64000),_throwErr(error.DES_ERR));const repnKey=_makeKey(keys.repn,repnId,apt);const repnValue=_checkExist(repnKey,error.REPN_NOT_EST);const repn=_toJsn(repnValue);const acpKey=_makeKey(keys.acp,repn.acceptanceId);const acpVal=_load(acpKey);const acp=_toJsn(acpVal);const acpAddr=Utils.toAddress(acp.publicKey);Utils.assert(gMsgSender===apt||_checkIsSeller(gMsgSender)||gMsgSender===acpAddr,_throwErr(error.NOT_APT_SEL_APR));const dptKey=_makeKey(keys.dpt,repnId,apt);_checkExist(dptKey,error.DPT_NO_EST);const eviKey=_makeKey(keys.evi,repnId,apt,gMsgSender);let evi={};evi.description=des;evi.addition=addi;_store(eviKey,_toStr(evi));Chain.tlog('setEvidence',_makeTlogSender(),repnId,apt);}function evidenceInfo(repnId,apt,pvr){Utils.assert(_checkStr(repnId,1,32),_throwErr(error.REPN_ID_ERR));Utils.assert(_checkAddr(apt),_throwErr(error.APT_ERR));Utils.assert(_checkAddr(pvr),_throwErr(error.PVR_ERR));const eviKey=_makeKey(keys.evi,repnId,apt,pvr);const eviVal=_checkExist(eviKey,error.EVI_NOT_EST);return eviVal;}function handleDispute(repnId,apt,sas,des){Utils.assert(_checkStr(repnId,1,32),_throwErr(error.REPN_ID_ERR));Utils.assert(_checkAddr(apt),_throwErr(error.APT_ERR));Utils.assert(sas===1||sas===2,_throwErr(error.SAS_ERR));Utils.assert(_checkStr(des,0,64000),_throwErr(error.DES_ERR));const repnKey=_makeKey(keys.repn,repnId,apt);const repnVal=_checkExist(repnKey,error.REPN_NOT_EST);let repn=_toJsn(repnVal);const dptKey=_makeKey(keys.dpt,repnId,apt);const dptVal=_checkExist(dptKey,error.DPT_NO_EST);let dpt=_toJsn(dptVal);Utils.assert(dpt.controller===gMsgSender,_throwErr(error.SENDER_NOT_CTR));Utils.assert(dpt.status===0,_throwErr(error.DPT_FSH));dpt.status=sas;_store(dptKey,_toStr(dpt));if(sas===1){repn.status=3;}else if(sas===2){repn.status=2;}_store(repnKey,_toStr(repn));Chain.tlog('handleDispute',_makeTlogSender(),repnId,apt,sas);}function disputeInfo(repnId,apt){Utils.assert(_checkStr(repnId,1,32),_throwErr(error.REPN_ID_ERR));Utils.assert(_checkAddr(apt),_throwErr(error.APT_ERR));const dptKey=_makeKey(keys.dpt,repnId,apt);const aptVal=_checkExist(dptKey,error.DPT_NO_EST);return aptVal;}function setSeller(cpyFlNm,cpyStNm,cpyCat,cpyCer){const isSel=_checkIsSeller(gTxSender);Utils.assert(isSel,_throwErr(error.NOT_SEL));_setSeller(gTxSender,cpyFlNm,cpyStNm,cpyCat,cpyCer);Chain.tlog('setSeller',_makeTlogSender(),cpyFlNm,cpyStNm,cpyCat);}function contractInfo(){return _load(_makeKey(keys.crt));}function init(bar){let params=_toJsn(bar);_setSeller(gTxSender,params.companyFullName,params.companyShortName,params.companyContact,params.companyCertification);let crt={};crt.name="ATP61";crt.version='1.0';_store(keys.crt,_toStr(crt));const dftTrn={};_store(_makeKey(keys.trn,'0'),_toStr(dftTrn));Chain.tlog('init',_makeTlogSender(),params.companyFullName,params.companyShortName,params.companyContact);}function main(input){const data=_toJsn(input);const method=data.method||'';const params=data.params||{};switch(method){case'setSeller':setSeller(params.companyFullName,params.companyShortName,params.companyContact,params.companyCertification);break;case'setDocument':setDocument(params.id,params.name,params.url,params.hashType,params.hash);break;case'createSpu':createSpu(params.id,params.name,params.type,params.attributes);break;case'setSpu':setSpu(params.spuId,params.name,params.type,params.attributes);break;case'issue':issue(params.skuId,params.trancheId,params.isDefaultTranche,params.spuId,params.name,params.symbol,params.faceValue,params.tokenId,params.tokenInfo,params.mainIcon,params.viceIcons,params.labels,params.description,params.redemptionAddress,params.acceptanceId,params.abstracts,params.attributes);break;case'setSkusChoice':setSkusChoice(params.spuId,params.choice);break;case'setSku':setSku(params.skuId,params.name,params.symbol,params.mainIcon,params.viceIcons,params.labels,params.description,params.redemptionAddress,params.abstracts,params.attributes);break;case'addAcceptanceToSku':addAcceptanceToSku(params.skuId,params.acceptanceId);break;case'delAcceptanceFromSku':delAcceptanceFromSku(params.skuId,params.acceptanceId);break;case'additionalIssuance':additionalIssuance(params.skuId,params.trancheId,params.tokenId,params.tokenInfo);break;case'assignToTranche':assignToTranche(params.skuId,params.toTrancheId,params.tokenId);break;case'setAuthorizers':setAuthorizers(params.skuId,params.authorizers);break;case'authorizeSku':authorizeSku(params.skuId,params.trancheId);break;case'createTranche':createTranche(params.id,params.description,params.limits);break;case'destroy':destroy(params.address,params.tokenId);break;case'transferFrom':transferFrom(params.from,params.to,params.tokenId);break;case'transfer':transfer(params.to,params.tokenId);break;case'approve':approve(params.spender,params.tokenId);break;case'setAcceptance':setAcceptance(params.id,params.publicKey,params.fullName,params.shortName,params.logo,params.contact,params.period,params.addition);break;case'requestRedemption':requestRedemption(params.redemptionId,params.tokenId,params.acceptanceId,params.description,params.addition);break;case'redeem':redeem(params.redemptionId,params.applicant);break;case'confirmRedemption':confirmRedemption(params.redemptionId,params.applicant);break;case'applyDispute':applyDispute(params.redemptionId,params.applicant,params.reason,params.controller,params.addition);break;case'handleDispute':handleDispute(params.redemptionId,params.applicant,params.status,params.description);break;case'setEvidence':setEvidence(params.redemptionId,params.applicant,params.description,params.addition);break;default:throw _throwErr(error.MTD_ERR);}}function query(input){const data=_toJsn(input);const method=data.method||'';const params=data.params||{};let val=null;switch(method){case'contractInfo':val=contractInfo();break;case'sellerInfo':val=sellerInfo();break;case'allDocuments':val=allDocuments();break;case'skusOfTranche':val=skusOfTranche(params.trancheId);break;case'tranchesOf':val=tranchesOf(params.skuId,params.address);break;case'acceptancesOfSku':val=acceptancesOfSku(params.skuId);break;case'acceptanceInfo':val=acceptanceInfo(params.acceptanceId);break;case'documentInfo':val=documentInfo(params.documentId);break;case'spuInfo':val=spuInfo(params.spuId);break;case'tokenInfo':val=tokenInfo(params.skuId);break;case'totalSupplyByTranche':val=totalSupplyByTranche(params.skuId,params.trancheId);break;case'trancheInfo':val=trancheInfo(params.trancheId);break;case'authorizedSku':val=authorizedSku(params.skuId,params.trancheId,params.authorizer);break;case'skusOfSpu':val=skusOfSpu(params.spuId);break;case'tranchesOfSku':val=tranchesOfSku(params.skuId);break;case'balanceOf':val=balanceOf(params.address,params.skuId);break;case'balanceOfByTranche':val=balanceOfByTranche(params.address,params.skuId,params.trancheId);break;case'allowance':val=allowance(params.owner,params.skuId,params.trancheId,params.spender);break;case'redemptionInfo':val=redemptionInfo(params.redemptionId,params.applicant);break;case'disputeInfo':val=disputeInfo(params.redemptionId,params.applicant);break;case'evidenceInfo':val=evidenceInfo(params.redemptionId,params.applicant,params.provider);break;default:throw _throwErr(error.MTD_ERR);}return val;}